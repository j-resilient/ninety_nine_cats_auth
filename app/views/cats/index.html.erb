<h1>Cats upon cats!</h1>
<%= render 'shared/errors' %>
<% unless current_user.nil? %>
  <h2> <%= "Hello, " + current_user.username %> </h2>
<% end %>
<ul>
  <% @cats.each do |cat| %>
    <li><a href="<%= cat_url(cat) %>"><%= cat.name %></a></li>
  <% end %>
</ul>

<a href="<%= new_cat_url %>">Create cat</a>
